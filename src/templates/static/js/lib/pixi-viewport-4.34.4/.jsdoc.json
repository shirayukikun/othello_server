{
    "applicationName": "pixi-viewport",
    "disqus": "",
    "openGraph": {
        "title": "pixi-viewport",
        "type": "website",
        "image": "",
        "site_name": "",
        "url": ""
    },
    "meta": {
        "title": "pixi-viewport",
        "description": "",
        "keyword": ""
    },
    "linenums": true,
    "source": {
        "include": [
            "./js",
            "./node_modules/eventemitter3"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "readme": "./README.md",
        "encoding": "utf8",
        "recurse": true,
        "private": false,
        "lenient": true,
        "destination": "./docs/jsdoc",
        "template": "./node_modules/yy-jsdoc-template"
    },
    "plugins": [
        "./node_modules/yy-jsdoc-template/plugins/skip-node_modules",
        "plugins/markdown"
    ],
    "readmeShowNav": "Viewport",
    "templates": {
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "pixi-viewport",
        "footer": "by YOPEY YOPEY LLC (yopeyopey.com)",
        "copyright": "Copyright Â© 2019 YOPEY YOPEY LLC.",
        "meta": {
            "title": "pixi-viewport API Documentation",
            "description": "Documentation for pixi-viewport library",
            "keyword": "docs, documentation, html5, javascript, jsdoc, camera, viewport, bounce, snap, 2d"
        }
    }
}